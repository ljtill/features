{
    "id": "uv",
    "version": "1.0.4",
    "name": "uv",
    "description": "Command line tool (uv)",
    "documentationURL": "https://docs.astral.sh/uv/",
    "options": {
        "version": {
            "type": "string",
            "description": "Select version of uv",
            "default": "latest"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "charliermarsh.ruff"
            ]
        }
    },
    "containerEnv": {
        "UV_LINK_MODE": "copy"
    }
}
