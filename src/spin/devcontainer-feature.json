{
    "id": "spin",
    "version": "1.1.2",
    "name": "Spin",
    "description": "Command line tool (spin)",
    "documentationURL": "https://developer.fermyon.com/spin",
    "options": {
        "version": {
            "type": "string",
            "description": "Select version of spin",
            "default": "latest"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "fermyon.spin-vscode"
            ],
            "settings": {
                "spin.customProgramPath": "/usr/local/bin/spin"
            }
        }
    }
}