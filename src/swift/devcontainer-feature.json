{
    "id": "swift",
    "version": "1.1.0",
    "name": "Swift",
    "description": "Command line tool (swift)",
    "documentationURL": "https://www.swift.org",
    "options": {
        "version": {
            "type": "string",
            "description": "Select version of swift",
            "default": "latest"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "swiftlang.swift-vscode"
            ]
        }
    },
    "containerEnv": {
        "SWIFTLY_HOME_DIR": "/opt/swiftly",
        "SWIFTLY_BIN_DIR": "/usr/local/bin"
    }
}