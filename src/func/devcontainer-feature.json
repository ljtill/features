{
    "id": "func",
    "version": "1.0.4",
    "name": "Azure Functions Core Tools",
    "description": "Command line tool (func)",
    "documentationURL": "https://learn.microsoft.com/azure/azure-functions/functions-run-local",
    "options": {
        "version": {
            "type": "string",
            "description": "Select version of func",
            "default": "latest"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-azuretools.vscode-azurefunctions"
            ]
        }
    },
    "containerEnv": {
        "DOTNET_SYSTEM_GLOBALIZATION_INVARIANT": "1"
    }
}
