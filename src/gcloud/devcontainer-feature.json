{
    "id": "gcloud",
    "version": "1.1.0",
    "name": "Google Cloud",
    "description": "Command line tool (gcloud)",
    "documentationURL": "https://cloud.google.com",
    "options": {
        "version": {
            "type": "string",
            "description": "Select version of gcloud",
            "default": "latest"
        },
        "update": {
            "type": "boolean",
            "description": "Update gcloud components",
            "default": true
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "googlecloudtools.cloudcode"
            ]
        }
    }
}
